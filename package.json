{
  "name": "@architecture/vamos-capo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"nodemon --esm src/server/index.ts\" \"vite --open\"",
    "server": "nodemon --esm src/server/index.ts",
    "build": "vite build",
    "lint": "eslint --config=.ci/linters/.eslintrc.cjs --ignore-path=.ci/linters/.eslintignore ./ --ext .js,.jsx,.mjs,.d.ts,.ts,.tsx",
    "lint:fix": "eslint --fix --config=.ci/linters/.eslintrc.cjs --ignore-path=.ci/linters/.eslintignore ./ --ext .js,.jsx,.mjs,.d.ts,.ts,.tsx",
    "preview": "vite preview",
    "prettier": "prettier --config=.ci/linters/prettier.config.cjs --ignore-path=.ci/linters/.prettierignore --list-different",
    "prettier:lint": "yarn prettier '**/*.{js?(on),ts?(x),scss,md,y?(a)ml,gql,graphql?(s),less,mjml}' --write=false",
    "prettier:fix": "yarn prettier:lint --write=true",
    "plop:client": "plop --plopfile provision/plop/plopfile.cjs",
    "test:client": "vitest",
    "start": "tsx src/server"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^5.3.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/node": "^7.61.1",
    "@sentry/tracing": "^7.61.1",
    "concurrently": "^8.2.0",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.14.2",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^22.1.0",
    "plop": "^2.7.6",
    "prettier": "^2.3.2",
    "prettier-eslint": "^12.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^0.34.1"
  },
  "prettier": "./.ci/linters/prettier.config.cjs"
}
